{
	"options": {
      "show-model-annotation": true,
      "minimize-model-annotation": true,
	},
	"services": {
		"restAPI": {
			"classname":"aRestAPI",
		},
		"aeWamManager": {
			"classname":"aeWamManager",
		},
		"compiler": {
			"classname":"aSystemCompilerService",
		},
		"aMultiUserManager": {
			"classname":"aMultiUserManager",
		},
		"http-service": {
			"classname":"aSystemHttpService",
			"url":"http://+:8082/",
			"map": {
				"/api/rest/": {
               "method" : "aeWamManager/ewamex" 
            }, 
				"/": {
               "method":"http-service/interpret-http-transaction"
            }
			}
		}
	}
}